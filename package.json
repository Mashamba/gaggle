{
  "name": "gaggle",
  "version": "1.2.5",
  "description": "Mutual exclusion made easy",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "tape test/**/*.js | tap-spec",
    "coverage": "nyc tape test/**/*.js --reporter=html && open -a \"Safari\" coverage/index.html",
    "test-ci": "nyc tape test/**/*.js && nyc report --reporter=text-lcov | coveralls",
    "tdd": "nodemon -x npm test",
    "doctoc": "doctoc README.md",
    "prepublish": "in-publish && npm run doctoc || not-in-publish",
    "benchmark": "node benchmark",
    "benchmarks": "for run in {1..10}\n do\n npm run benchmark || true \n  done",
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ben-ng/gaggle.git"
  },
  "keywords": [
    "distributed",
    "mutal",
    "exclusion",
    "mutex",
    "lock",
    "semaphore",
    "maekawa",
    "lamport",
    "transaction",
    "ricart",
    "agrawala"
  ],
  "author": "Ben Ng <me@benng.me>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ben-ng/gaggle/issues"
  },
  "homepage": "https://github.com/ben-ng/gaggle#readme",
  "devDependencies": {
    "async": "^1.5.2",
    "benchmark": "^2.0.0",
    "coveralls": "^2.11.6",
    "doctoc": "^0.15.0",
    "joi": "^6.6.1",
    "jstrace-bars": "^1.2.3",
    "nodemon": "^1.4.1",
    "nyc": "^5.3.0",
    "once": "^1.3.3",
    "tap-spec": "^4.1.1",
    "tape": "^4.2.0"
  },
  "dependencies": {
    "bluebird": "^2.9.34",
    "in-publish": "^2.0.0",
    "lodash": "^4.0.0",
    "redis": "^2.4.2",
    "uuid": "^2.0.1"
  }
}
