  },
  "homepage": "https://github.com/ben-ng/gaggle#readme",
  "devDependencies": {
    "async": "^1.5.2",
    "covert": "^1.1.0",
    "faucet": "0.0.1",
    "joi": "^6.6.1",
    "nodemon": "^1.4.1",
    "once": "^1.3.3",
    "tape": "^4.2.0"
  },
  "dependencies": {
