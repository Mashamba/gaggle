{
  "name": "gaggle",
  "version": "1.0.3",
  "description": "Mutual exclusion made easy",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "covert test/*/*/*.js | faucet",
    "test-verbose": "covert test/*/*/*.js",
    "test-nocov": "tape test/*/*/*.js",
    "tdd": "nodemon -x npm test"
